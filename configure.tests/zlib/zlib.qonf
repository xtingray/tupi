<?xml version="1.0" ?>
<qonf optional="false" >
	<test name="Zlib">
		<dir path="." />
	</test>
	<provide>
		<defines>
			<define value="HAVE_ZLIB" />
		</defines>
		<libs>
			<lib path="-lz" />
		</libs>
	</provide>
	<solution>
		<linux package="aspell" url="http://zlib.net/" comment="" >
			<distribution name="gentoo" package="zlib" url="http://zlib.net/" comment="Try: emerge zlib" />
			<distribution name="ubuntu" package="zlib1g-dev" url="http://zlib.net/" comment="Try: apt-get install zlib1g-dev" />
		</linux>
		<windows package="zlib" url="http://zlib.net/" comment="" />
		<macosx package="zlib" url="http://zlib.net/" comment="" />
	</solution>
</qonf>
