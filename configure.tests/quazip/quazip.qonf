<?xml version="1.0" ?>
<qonf optional="false" >
	<test name="QuaZIP">
		<dir path="." />
	</test>
	<provide>
		<defines>
			<define value="HAVE_QUAZIP" />
		</defines>
		<libs>
			<lib path="-lquazip" />
		</libs>
                <includes>
                        <include path="/usr/include/quazip/" />
                </includes>
	</provide>
	<solution>
		<linux package="quazip" url="http://quazip.sourceforge.net/" comment="" >
			<distribution name="gentoo" package="quazip" url="http://quazip.sourceforge.net/" comment="Try: emerge quazip" />
			<distribution name="ubuntu" package="quazip" url="http://quazip.sourceforge.net/" comment="Try: apt-get install quazip" />
		</linux>
		<windows package="quazip" url="http://quazip.sourceforge.net/" comment="" />
		<macosx package="quazip" url="http://quazip.sourceforge.net/" comment="" />
	</solution>
</qonf>
